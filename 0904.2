SELECT EX.NOME, FX.NOME
FROM TabelaEX AS EX
INNER JOIN TabelaFX AS FX ON EX.NOME = FX.nome

SELECT EX.NOME, FX.NOME
FROM TabelaEX AS EX
LEFT JOIN TabelaFX AS FX ON EX.NOME = FX.nome


SELECT EX.NOME, FX.NOME
FROM TabelaEX AS EX
RIGHT JOIN TabelaFX AS FX ON EX.NOME = FX.nome

